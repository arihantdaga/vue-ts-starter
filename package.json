    "grunt-contrib-jshint": "^1.1.0",
    "grunt-contrib-nodeunit": "^1.0.0",
    "grunt-run": "^0.8.0",
    "html-webpack-plugin": "^2.30.1",
    "http-server": "~0.10.0",
    "istanbul-instrumenter-loader": "~3.0.0",
    "json-loader": "~0.5.7",
