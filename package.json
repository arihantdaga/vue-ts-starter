{
  "name": "vue-ts-starter",
  "description": "",
  "version": "1.0.0",
  "author": "Arihant Daga <arihant.ekism@gmail.com>",
  "private": true,
  "engines": {
    "node": ">=6",
    "npm": ">=3"
  },
  "bin": {
    "run_deploy_to_server": "./bin/run_deploy_to_server.js"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production npm run clean &&  npm run compile",
    "ci:teamcity": "karma --env=tc start karma.coverage.js && npm run coverage:remap",
    "ci:jenkins": "karma --env=jk start karma.coverage.js && npm run coverage:remap",
    "clean": "rimraf dist && rimraf coverage",
    "compile": "webpack --config config/webpack.config.prod.js --bail",
    "coverage": "npm run coverage:run && npm run coverage:remap && npm run coverage:open",
    "coverage:open": "opn coverage/html-ts/index.html",
    "coverage:remap": "remap-istanbul -i coverage/json/coverage-final.json -o coverage/html-ts -t html",
    "coverage:run": "cross-env NODE_ENV=development karma start karma.coverage.js",
    "dev": "webpack-dev-server --hot --inline",
    "dev_all": "webpack-dev-server --hot --inline --host 0.0.0.0 --port 3000",
    "lint": "tslint src/**/*.ts",
    "serve": "http-server dist/ -g -o",
    "test": "cross-env NODE_ENV=development karma start karma.unit.js",
    "test:debug": "cross-env NODE_ENV=development karma start karma.debug.js",
    "test:watch": "cross-env NODE_ENV=development karma start karma.unit.js --singleRun=false --auto-watch",
    "build_deploy": "node deploy_to_server",
    "push": "npm run build_deploy && git add --all && git commit -m \" (Build) New Build \" && git push origin master",
    "start_server": "cross-env NODE_ENV=production PORT=4070 node server/index.js"
  },
  "dependencies": {
    "autosize": "^4.0.1",
    "axios": "^0.17.1",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.0.0",
    "bootstrap-vue": "^2.0.0-rc.6",
    "cookie-parser": "^1.4.3",
    "ejs": "^2.5.7",
    "helmet": "^3.13.0",
    "html2plaintext": "^2.0.1",
    "lodash": "^4.17.5",
    "medium-editor": "^5.23.3",
    "method-override": "^3.0.0",
    "microtip": "^0.2.2",
    "morgan": "^1.9.1",
    "nprogress": "^0.2.0",
    "prerender-spa-plugin": "^3.4.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "sweetalert": "git+https://github.com/t4t5/sweetalert.git",
    "uiv": "^0.15.5",
